<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                      https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="XGuard\Bot\Facebook\Domain\Entity\ChannelEntity" table="f_channel">
        <id name="id" column="id" type="UuidType"/>
        <field name="channel_id" column="channel_id" type="ChannelIdType"/>
        <field name="name" column="name" type="ChannelNameType"/>
        <field name="type" column="type" type="TypeType"/>
    </entity>

</doctrine-mapping>
