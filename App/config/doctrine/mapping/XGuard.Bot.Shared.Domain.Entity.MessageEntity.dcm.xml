<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                      https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="XGuard\Bot\Shared\Domain\Entity\MessageEntity" table="s_message">
        <id name="id" column="id" type="UuidType"/>
        <field name="date_create" column="date_create" type="DateType"/>
        <field name="channel" column="channel" type="UuidType"/>
        <field name="text" column="text" type="TextType"/>
    </entity>

</doctrine-mapping>
