<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                      https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="XGuard\Bot\Telegram\Domain\Entity\ChannelEntity" table="t_channel">
        <id name="id" column="id" type="UuidType"/>
        <field name="chat_id" column="chat_id" type="ChatIdType"/>
        <field name="name" column="name" type="ChannelNameType"/>
    </entity>

</doctrine-mapping>
